<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/darkergrey" />
            <size android:width="100dp" android:height="100dp"></size>
            <stroke android:width="1dp" android:color="@color/white" />
        </shape>
    </item>


    <item android:gravity="center"
        android:start="14dp"
        android:end="14dp"
        android:top="14dp"
        android:bottom="14dp"
        >
        <shape android:shape="oval" >
            <solid android:color="@color/darkergrey"/>
            <stroke android:color="@color/holo_blue_bright" android:width="5dp"></stroke>
            <size android:width="40dp" android:height="40dp"></size>
        </shape>

    </item>
</layer-list>